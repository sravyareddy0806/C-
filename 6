#include <iostream>
#include <cstdio>
#include <algorithm> // Required for std::max

using namespace std;

/*
Add `int max_of_four(int a, int b, int c, int d)` here.
*/
int max_of_four(int a, int b, int c, int d) {
    // Start by assuming 'a' is the largest
    int max_val = a;
    
    // Compare max_val with b, c, and d sequentially
    if (b > max_val) {
        max_val = b;
    }
    if (c > max_val) {
        max_val = c;
    }
    if (d > max_val) {
        max_val = d;
    }
    
    return max_val;
}

int main() {
    int a, b, c, d;
    // Reading four integers
    if (scanf("%d %d %d %d", &a, &b, &c, &d) == 4) {
        int ans = max_of_four(a, b, c, d);
        printf("%d\n", ans);
    }
    return 0;
}
