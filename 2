#include <iostream>

using namespace std;

int main() {
    // Declare three variables to store the input integers
    int a, b, c;
    
    // Read three space-separated integers from stdin
    cin >> a >> b >> c;
    
    // Calculate the sum and print it to stdout
    // Using endl ensures the output ends with a newline
    cout << a + b + c << endl;
    
    return 0;
}
