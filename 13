#include <iostream>
#include <string>
#include <sstream>

using namespace std;

class Student {
private:
    // Member variables (Attributes)
    int age;
    int standard;
    string first_name;
    string last_name;

public:
    // Setter methods
    void set_age(int a) {
        age = a;
    }
    
    void set_first_name(string fn) {
        first_name = fn;
    }
    
    void set_last_name(string ln) {
        last_name = ln;
    }
    
    void set_standard(int s) {
        standard = s;
    }

    // Getter methods
    int get_age() {
        return age;
    }
    
    string get_first_name() {
        return first_name;
    }
    
    string get_last_name() {
        return last_name;
    }
    
    int get_standard() {
        return standard;
    }

    // Returns a comma-separated string of all attributes
    string to_string() {
        stringstream ss;
        // Using stringstream to concatenate different types
        ss << age << "," << first_name << "," << last_name << "," << standard;
        return ss.str();
    }
};

int main() {
    int age, standard;
    string first_name, last_name;
    
    // Reading input
    cin >> age >> first_name >> last_name >> standard;
    
    // Creating object and setting values
    Student st;
    st.set_age(age);
    st.set_first_name(first_name);
    st.set_last_name(last_name);
    st.set_standard(standard);
    
    // Outputting values using getters
    cout << st.get_age() << "\n";
    cout << st.get_last_name() << ", " << st.get_first_name() << "\n";
    cout << st.get_standard() << "\n";
    cout << "\n"; // Blank line as per sample output
    cout << st.to_string() << "\n";
    
    return 0;
}
